<% _.each(projects, function(project){ %>
  <!-- <ul class="index-list dontsplit"> -->
    <li class="dontend"><a href="<%= url_from_project_name(project.idx, project.project_name_en) %>" data-permalink="<%= project.idx %>-<%= conv_to_slug(project.project_name_en) %>" class="participant_change_btn">(<%= project.idx %>)</a></li>
    <% _.each(project.project_artists, function(artist) { %>
      <li>
        <a href="<%= make_url(artist.url) %>" data-permalink="<%= extract_permalink_from_url(artist.url) %>" class="participant_change_btn">
          <% if (WY.constants.locale == "ko") { %>
            <span lang="ko"><%= artist.full_name_ko %></span>
          <% } else { %>
            <span lang="en"><%= artist.full_name_en %></span>
          <% } %>
        </a>
      </li>
    <% }); %>
    <li class="removeiflast removeiffirst">&nbsp;</li>
  <!-- </ul> -->
<% }); %>
