<ul class="index-list">
  <% _.each(projects, function (project) { %>
    <li>
      <a href="#">
        (<%= project.idx %>) 
        <% if (WY.constants.locale == "ko") { %>
          <span lang="ko"><%= project.project_name_kr %></span>
        <% } else { %>
          <span lang="en"><%= project.project_name_en %></span>
        <% } %>
      </a>
    </li>
  <% }); %>
</ul>